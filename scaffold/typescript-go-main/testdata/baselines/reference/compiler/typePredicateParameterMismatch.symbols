// references a parameter name that doesn't match any actual function parameter.

type TypeA = { kind: 'a' };
>TypeA : Symbol(TypeA, Decl(typePredicateParameterMismatch.ts, 0, 0))
>kind : Symbol(kind, Decl(typePredicateParameterMismatch.ts, 3, 14))

